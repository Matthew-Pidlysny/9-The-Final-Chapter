    "encryption": true,
    "threshold_gb": 1.0
  },
  "ai_integration": {
    "enabled": true,
    "service": "openai_gpt",
    "max_tokens": 4000,
    "temperature": 0.3
  },
  "visualization": {
    "root_compatible": true,
    "graph_resolution": "high",
    "export_formats": [
      "png",
      "svg",
      "pdf"
    ],
    "interactive_plots": true
